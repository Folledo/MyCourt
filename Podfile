project 'FolledoMyCourt.xcodeproj'

# Uncomment the next line to define a global platform for your project
 platform :ios, '11.0'

target 'FolledoMyCourt' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FolledoMyCourt
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  
  pod 'Firebase/AdMob'
  
  pod 'SDWebImage', '~> 4.0'
  
  
  pod 'SwiftyStoreKit'
  
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'

  target 'FolledoMyCourtTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FolledoMyCourtUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
